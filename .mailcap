# $Id$

text/html; luakit openURL\(%s\)
image/gif; xv %s;
image/jpg; feh -. '%s';
image/jpeg; feh -. '%s';
image/png; feh -. '%s';
application/pgp-keys; pgp -f < %s ; copiousoutput
application/msword; libreoffice '%s'; copiousoutput; description=Word Document;nametemplate=%s.doc
application/msword; antiword '%s'; copiousoutput;
application/vnd.oasis.opendocument.text; libreoffice '%s'; copiousoutput; description=Word Document; 
application/pdf; jfbview %s;
application/pdf; evince %s;
text/html;      w3m -dump %s; nametemplate=%s.html; copiousoutput
